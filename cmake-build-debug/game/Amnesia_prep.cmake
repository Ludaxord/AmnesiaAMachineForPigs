include(BundleUtilities)
get_bundle_and_executable("${BUNDLE_APP}" bundle executable valid)
if(valid)
  set(framework_path "${bundle}/Contents/Frameworks")
  foreach(framework )
    file(MAKE_DIRECTORY "${framework_path}/${framework}.framework/Versions/A/")
    copy_resolved_framework_into_bundle(${DEP_LIB_DIR}/${framework}.framework/Versions/A/${framework} ${framework_path}/${framework}.framework/Versions/A/${framework})
  endforeach()
  foreach(lib libSDL2.dylib;libIL.1.dylib)
    copy_resolved_item_into_bundle(${DEP_LIB_DIR}/${lib} ${framework_path}/${lib})
  endforeach()
else()
  message(ERROR "App Not found? ${BUNDLE_APP}")
endif()
